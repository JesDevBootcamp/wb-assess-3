{% extends "base.html.njk" %}
{% block title %}
	Most Liked Fossils
{% endblock %}
{% block content %}
	<h1>Welcome, {{ userName }}</h1>
	{% for key, fossil in fossils %}
	<div class="fossil-div">
		<h3>{{ userName }}, this is a {{ fossil.name }}</h3>
		<h2>Liked {{ fossil.num_likes }} times.</h2>
		<img src="{{ fossil.img }}" />
	</div>
	{% endfor %}
{% endblock %}